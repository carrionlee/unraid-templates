<?xml version="1.0"?>
<Container version="2">
  <Name>CloudDrive</Name>
  <Repository>cloudnas/clouddrive</Repository>
  <Registry>https://hub.docker.com/r/cloudnas/clouddrive/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/cloudnas/clouddrive/</Support>
  <Project/>
  <Overview> Clouddrive mounts cloud storage services as local file system. </Overview>
  <Category>Cloud:</Category>
  <WebUI>http://[IP]:[PORT:9798]</WebUI>
  <TemplateURL/>
  <Icon>https://i.loli.net/2021/08/15/86P9H2bO7ZdWerN.jpg</Icon>
  <ExtraParams>--cap-add SYS_ADMIN --security-opt apparmor:unconfine</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1629014426</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description> Clouddrive mounts cloud storage services as local file system. </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9798</HostPort>
        <ContainerPort>9798</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir/>
      <ContainerDir>/Config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir/>
      <ContainerDir>/CloudNAS</ContainerDir>
      <Mode>rw,shared</Mode>
    </Volume>
    <Volume>
      <HostDir/>
      <ContainerDir>/media</ContainerDir>
      <Mode>rw,shared</Mode>
    </Volume>
  </Data>
  <Environment/>
  <Labels/>
  <Config Name="Host Path 1" Target="/Config" Default="" Mode="rw" Description="Container Path: /Config" Type="Path" Display="always" Required="false" Mask="false"/>
  <Config Name="Host Port 1" Target="9798" Default="9798" Mode="tcp" Description="Container Port: 9798" Type="Port" Display="always" Required="false" Mask="false">9798</Config>
  <Config Name="Host Path 2" Target="/CloudNAS" Default="" Mode="rw,shared" Description="Container Path: /CloudNAS" Type="Path" Display="always" Required="false" Mask="false"/>
  <Config Name="Host Path 3" Target="/media" Default="" Mode="rw,shared" Description="Container Path: /media" Type="Path" Display="always" Required="false" Mask="false"/>
  <Config Name="/dev/fuse" Target="" Default="" Mode="" Description="Container Device: " Type="Device" Display="always" Required="false" Mask="false">/dev/fuse</Config>